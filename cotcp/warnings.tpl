<!-- BEGIN: ERROR -->
<div class="alert alert-danger py-3">
    <h4 class="mb-2">{PHP.L.Error}</h4>
    <ul class="mb-0">
        <!-- BEGIN: ERROR_ROW -->
        <li>{ERROR_ROW_MSG}</li>
        <!-- END: ERROR_ROW -->
    </ul>
</div>
<!-- END: ERROR -->

<!-- BEGIN: WARNING -->
<div class="alert alert-warning py-3">
    <h4 class="mb-2">{PHP.L.Warning}</h4>
    <ul class="mb-0">
        <!-- BEGIN: WARNING_ROW -->
        <li>{WARNING_ROW_MSG}</li>
        <!-- END: WARNING_ROW -->
    </ul>
</div>
<!-- END: WARNING -->

<!-- BEGIN: DONE -->
<div class="alert alert-success py-3">
    <h4 class="mb-2">{PHP.L.Done}</h4>
    <ul class="mb-0">
        <!-- BEGIN: DONE_ROW -->
        <li>{DONE_ROW_MSG}</li>
        <!-- END: DONE_ROW -->
    </ul>
</div>
<!-- END: DONE -->